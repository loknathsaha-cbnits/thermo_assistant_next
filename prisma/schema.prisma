datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

model User {
  id              String  @id @default(cuid())
  name            String
  email           String  @unique
  password        String
  image           String?
  emailVerified   String?  @default("false")
}
